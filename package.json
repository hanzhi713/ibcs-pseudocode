{
    "name": "ibcs-pseudocode",
    "displayName": "IBCS Pseudocode",
    "description": "Basic syntax highlight support for IB-styled pseudocode",
    "version": "0.1.0",
    "publisher": "hanzhi713",
    "repository": {
        "type": "git",
        "url": "https://github.com/hanzhi713/ibcs-pseudocode.git"
    },
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "pseudo",
                "aliases": [
                    "Pseudocode",
                    "pseudo"
                ],
                "extensions": [
                    ".pseudo"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "pseudo",
                "scopeName": "source.pseudo",
                "path": "./syntaxes/pseudo.tmLanguage.json"
            }
        ]
    }
}
